CREATE TABLE HR.LOGINUSER(USER_FIRST_NAME varchar2(50) NOT NULL,
  USER_LAST_NAME VARCHAR2(50) NOT NULL,
  USER_USERNAME VARCHAR2(50) NOT NULL,
  USER_PASSWORD VARCHAR2(50) NOT NULL,
